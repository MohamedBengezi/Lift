{
  "expo": {
    "name": "Lift",
    "slug": "social",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme":"liftapp",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capstone.lift",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.capstone.lift",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{
            "scheme": "liftapp"
          }],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}